# API Keys Configuration
# Copy this file to .env and replace placeholder values with your actual credentials

# Google Maps API Key (REQUIRED)
GoogleMaps__ApiKey=YOUR_GOOGLE_MAPS_API_KEY_HERE

# JWT Secret Key (REQUIRED)
Jwt__Key=YOUR_JWT_SECRET_KEY_HERE

# Email Configuration (Gmail SMTP)
Email__SmtpHost=smtp.gmail.com
Email__SmtpPort=587
Email__SenderEmail=YOUR_GMAIL_ADDRESS_HERE
Email__SenderName=TriLink
Email__AppPassword=YOUR_GMAIL_APP_PASSWORD_HERE
Email__LogoUrl=http://localhost:5000/trilink_logo.jpg

//If you want to use a hosted logo, replace with your URL:
//Email__LogoUrl=https://yourdomain.com/logo.png

# OTP Configuration
Otp__Length=6
Otp__ExpiryMinutes=10
Otp__MaxAttempts=3
Otp__RateLimitPerHour=3

# NOTE: Connection string is in appsettings.json


